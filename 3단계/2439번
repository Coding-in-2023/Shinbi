import java.util.Scanner;

public class Main {
public static void main(String args[]) {

Scanner sc = new Scanner(System.in);

	int N = sc.nextInt();
//행의 개수
	for (int i = 1; i <= N; i++) {
		//빈칸 찍기
		for (int j = 1; j<=N-i; j++) {
			System.out.print(" ");
		}
		//별의 찍기 
			for(int k=1;k<=i; k++)
			{
			  System.out.print("*");
			}
		
		System.out.println();
	}
	
}
}
